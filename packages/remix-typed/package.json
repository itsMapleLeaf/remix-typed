{
  "type": "module",
  "name": "remix-typed",
  "author": "itsMapleLeaf",
  "description": "End-to-end type safety with Remix",
  "version": "0.1.2",
  "license": "MIT",
  "repository": "https://github.com/itsMapleLeaf/remix-typed",
  "homepage": "https://github.com/itsMapleLeaf/remix-typed#readme",
  "changelog": "https://github.com/itsMapleLeaf/remix-typed/releases",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "scripts": {
    "build": "tsup-node src/main.ts --clean --target node16 --format cjs,esm --dts",
    "release": "release-it"
  },
  "dependencies": {
    "@types/react": "*",
    "@types/react-dom": "*"
  },
  "peerDependencies": {
    "@remix-run/react": ">=1.1.3",
    "@remix-run/server-runtime": ">=1.1.3",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "zod": ">=3.11.6"
  },
  "devDependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/server-runtime": "^1.1.3",
    "@types/node": "^17.0.14",
    "esbuild": "^0.14.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "release-it": "^14.12.4",
    "tsup": "^5.11.13",
    "zod": "^3.11.6"
  }
}
