{
  "type": "module",
  "name": "remix-typed",
  "author": "itsMapleLeaf",
  "description": "End-to-end type safety with Remix",
  "version": "0.1.2",
  "license": "MIT",
  "repository": "https://github.com/itsMapleLeaf/remix-typed",
  "homepage": "https://github.com/itsMapleLeaf/remix-typed#readme",
  "changelog": "https://github.com/itsMapleLeaf/remix-typed/releases",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "scripts": {
    "build": "tsup-node src/main.tsx --clean --target node16 --format esm,cjs --sourcemap --dts",
    "typecheck": "tsc --noEmit",
    "release": "release-it",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "pnpm lint -- --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@remix-run/server-runtime": "0.0.0-experimental-9784dd06",
    "@types/react": "*"
  },
  "peerDependencies": {
    "@remix-run/react": ">=1.5",
    "react": ">=17"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.3",
    "@remix-run/react": "0.0.0-experimental-9784dd06",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/eslint": "^8.4.3",
    "@types/node": "^17.0.42",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "prettier": "^2.7.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "release-it": "^15.0.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vitest": "^0.14.2"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
