{
  "type": "module",
  "name": "remix-typed",
  "author": "itsMapleLeaf",
  "description": "End-to-end type safety with Remix",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/itsMapleLeaf/remix-typed",
  "homepage": "https://github.com/itsMapleLeaf/remix-typed#readme",
  "changelog": "https://github.com/itsMapleLeaf/remix-typed/releases",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "scripts": {
    "build": "tsup-node src/main.ts --clean --target node16 --format cjs,esm --dts",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "pnpm lint -- --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.11.6",
    "@types/react": "*",
    "@types/react-dom": "*"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/server-runtime": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.2",
    "@remix-run/react": "^1.1.3",
    "@remix-run/server-runtime": "^1.1.3",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.18",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@itsmapleleaf/configs/eslint"
    ],
    "ignorePatterns": [
      "**/node_modules/**",
      "**/dist/**"
    ]
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
